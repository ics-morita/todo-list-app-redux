{
  "name": "todo-list-app-redux",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.20.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^7.2.1",
    "react-scripts": "3.4.3",
    "redux": "^4.0.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "json-server": "^0.16.1",
    "prettier": "2.0.5",
    "prettier-eslint": "11.0.0",
    "prettier-eslint-cli": "5.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"yarn run mock\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier-eslint --write $PWD/'src/**/*.{js,jsx,css}' && prettier-eslint --write $PWD/'*.config.js'",
    "mock": "json-server --watch ../api-mock/data.json --port 5000"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
